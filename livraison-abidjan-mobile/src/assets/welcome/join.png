
<svg width="300" height="200" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="joinGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#9C27B0;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#BA68C8;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="300" height="200" fill="#f8f9fa"/>
  
  <!-- Welcome hands -->
  <g transform="translate(100, 50)">
    <!-- Left hand -->
    <path d="M 20 30 Q 15 25 10 30 Q 8 35 12 40 L 25 45 Q 30 50 35 45 Q 40 40 35 35 L 22 32" fill="url(#joinGradient)"/>
    
    <!-- Right hand -->
    <path d="M 80 30 Q 85 25 90 30 Q 92 35 88 40 L 75 45 Q 70 50 65 45 Q 60 40 65 35 L 78 32" fill="url(#joinGradient)"/>
    
    <!-- Heart in the middle -->
    <path d="M 50 35 C 45 30, 35 30, 35 40 C 35 50, 50 60, 50 60 C 50 60, 65 50, 65 40 C 65 30, 55 30, 50 35 Z" fill="#FF5722"/>
  </g>
  
  <!-- Welcome text effect -->
  <g transform="translate(50, 120)">
    <rect x="0" y="0" width="200" height="30" fill="url(#joinGradient)" rx="15"/>
    <text x="100" y="20" text-anchor="middle" fill="#fff" font-family="Arial, sans-serif" font-size="14" font-weight="bold">REJOIGNEZ-NOUS</text>
  </g>
  
  <!-- Decorative elements -->
  <circle cx="50" cy="40" r="3" fill="#FFD700"/>
  <circle cx="250" cy="60" r="4" fill="#4CAF50"/>
  <circle cx="30" cy="120" r="2" fill="#2196F3"/>
  <circle cx="270" cy="140" r="3" fill="#FF5722"/>
  
  <!-- Title -->
  <text x="150" y="180" text-anchor="middle" fill="#333" font-family="Arial, sans-serif" font-size="16" font-weight="bold">Ensemble vers l'avenir</text>
</svg>
