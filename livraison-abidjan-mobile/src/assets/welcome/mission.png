
<svg width="300" height="200" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="missionGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#FF6B00;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#FF8F3C;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="300" height="200" fill="#f8f9fa"/>
  
  <!-- Target/Mission icon -->
  <g transform="translate(125, 40)">
    <!-- Outer circle -->
    <circle cx="25" cy="25" r="25" fill="none" stroke="url(#missionGradient)" stroke-width="4"/>
    <!-- Middle circle -->
    <circle cx="25" cy="25" r="15" fill="none" stroke="url(#missionGradient)" stroke-width="3"/>
    <!-- Inner circle -->
    <circle cx="25" cy="25" r="8" fill="url(#missionGradient)"/>
    
    <!-- Arrow pointing to center -->
    <path d="M 60 10 L 45 15 L 45 12 L 35 12 L 35 8 L 45 8 L 45 5 Z" fill="#333"/>
  </g>
  
  <!-- City buildings silhouette -->
  <g transform="translate(50, 110)">
    <rect x="0" y="20" width="30" height="40" fill="#ddd"/>
    <rect x="40" y="10" width="25" height="50" fill="#ccc"/>
    <rect x="75" y="25" width="35" height="35" fill="#ddd"/>
    <rect x="120" y="15" width="30" height="45" fill="#ccc"/>
    <rect x="160" y="30" width="40" height="30" fill="#ddd"/>
  </g>
  
  <!-- Title -->
  <text x="150" y="180" text-anchor="middle" fill="#333" font-family="Arial, sans-serif" font-size="16" font-weight="bold">Notre Mission</text>
</svg>
